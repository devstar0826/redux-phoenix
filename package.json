{
  "name": "redux-phoenix",
  "version": "1.1.0",
  "description": "Persist redux state",
  "repository": {
    "type": "git",
    "url": "https://github.com/adam-golab/redux-phoenix"
  },
  "main": "lib/redux-phoenix.js",
  "scripts": {
    "prepublish": "NODE_PATH=src npm run build",
    "prebuild": "rimraf ./lib",
    "build": "NODE_PATH=src rollup -c",
    "watch": "NODE_PATH=src rollup -c -w",
    "lint": "esw rollup.config.js src --color",
    "test": "NODE_PATH=src jest --no-cache",
    "test:watch": "NODE_PATH=src jest --watch --no-cache"
  },
  "keywords": [
    "redux",
    "persist",
    "store",
    "persist",
    "state"
  ],
  "author": "Adam Gołąb",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^5.1.0",
    "eslint-config-brainhub": "^1.9.0",
    "eslint-watch": "^4.0.1",
    "jest": "^23.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "sinon": "^6.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2"
  },
  "jest": {
    "testRegex": "/test/.*\\.js$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js?$": "<rootDir>/jest.transform.js"
    }
  }
}
